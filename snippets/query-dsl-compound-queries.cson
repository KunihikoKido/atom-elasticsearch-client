'.source.json':
  'Constant Score Query':
    'prefix': 'constant_score'
    'body': """
      "constant_score": {
        "filter": {
          ${1:"term": \\{"user": "kimchy"\\}}
        },
        "boost": ${2:1.2}
      }$3
    """
  'Bool Query':
    'prefix': 'bool'
    'body': """
      "bool": {
        "must": {
          ${1:"term": \\{"user": "kimchy"\\}}
        },
        "filter": {
          ${2:"term": \\{"tag": "tech"\\}}
        },
        "must_not": {
          ${3:"range": \\{"age": \\{"from": 10,"to": 20\\}\\}}
        },
        "should": [
          ${4:\\{"term": \\{"tag": "wow"\\}\\}},
          ${5:\\{"term": \\{"tag": "elasticsearch"\\}\\}}
        ],
        "minimum_should_match": ${6:1},
        "boost": ${7:1.0}
      }$8
    """
  'Dis Max Query':
    'prefix': 'dis_max'
    'body': """
      "dis_max": {
        "tie_breaker": ${1:0.7},
        "boost": ${2:1.2},
        "queries": [
          ${3:\\{"term": \\{"age": 34\\}\\}},
          ${4:\\{"term": \\{"age": 35\\}\\}}
        ]
      }$5
    """
  'Function Score Query':
    'prefix': 'function_score'
    'body': """
      "function_score": {
        "functions": [
          ${1:\\{"gauss": \\{"price": \\{"origin": "0", "scale": "20"\\}\\}\\}},
          ${2:\\{"gauss": \\{"location": \\{"origin": "11, 12", "scale": "2km"\\}\\}\\}}
        ],
        "query": {
          ${3:"match": \\{"properties": "balcony"\\}}
        },
        "score_mode": "${4:multiply}"
      }$5
    """
  'Boosting Query':
    'prefix': 'boosting'
    'body': """
      "boosting": {
        "positive": {
          ${1:"term": \\{"field1": "value1"\\}}
        },
        "negative": {
          ${2:"term": \\{"field2": "value2"\\}}
        },
        "negative_boost": ${3:0.2}
      }$4
    """
  'Indices Query':
    'prefix': 'indices'
    'body': """
      "indices": {
        "indices": [${1:"index1", "index2"}],
        "query": {
          ${2:"term": \\{"tag": "wow"\\}}
        },
        "no_match_query": {
          ${3:"term": \\{"tag": "kow"\\}}
        }
      }$4
    """
  'Filtered Query':
    'prefix': 'filtered'
    'body': """
      "filtered": {
        /*
          WARNING! Deprecated in 2.0.0-beta1.
          Use the bool query instead with a must clause
          for the query and a filter clause for the filter.
        */
        "query": {
          ${1:"match": \\{"tweet": "full text search"\\}}
        },
        "filter": {
          ${2:"range": \\{"created": \\{"gte": "now-1d/d"\\}\\}}
        }
      }$3
    """
  'Limit Query':
    'prefix': 'limit'
    'body': """
      "limit": {
        "value": ${1:100}
      }$2
    """
